{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es2017"],
    "module": "commonjs",
    "moduleResolution": "node",
    "strict": true,
    "alwaysStrict": true,
    "strictNullChecks": true,
    "noEmitOnError": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
  },
  "exclude": ["**/*.test.ts"]
}
